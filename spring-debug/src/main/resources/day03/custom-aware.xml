<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
	   http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="person" class="com.taozi.day03.custom.aware.Person" autowire="byType">
		<property name="id" value="1"/>
		<property name="name" value="桃子"/>
	</bean>
	<bean id="address" class="com.taozi.day03.entity.Address">
		<property name="province" value="辽宁"/>
		<property name="city" value="沈阳"/>
	</bean>
</beans>