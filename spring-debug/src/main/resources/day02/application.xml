<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
	   http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="person" class="com.taozi.day02.entity.Person">
<!--		<property name="id" value="1"/>-->
<!--		<property name="name" value="肇淘胜"/>-->
<!--		<property name="address" ref="address"/>-->
		<constructor-arg index="0" value="1"/>
		<constructor-arg type="java.lang.String" value="桃子"/>
		<constructor-arg name="address" ref="address"/>
	</bean>
	<bean id="address" class="com.taozi.day02.entity.Address">
		<property name="province" value="辽宁"/>
		<property name="city" value="沈阳"/>
	</bean>
</beans>